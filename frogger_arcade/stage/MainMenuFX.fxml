<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frogger_arcade.stage.MainMenuController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="609.0" layoutX="1.0" pickOnBounds="true">
         <image>
            <Image url="@../media/smiiling-big-eyed-green-frog-clipart-6926.jpg" />
         </image>
      </ImageView>
      <Button fx:id="startButton" layoutX="250.0" layoutY="148.0" mnemonicParsing="false" onAction="#startUp" prefHeight="61.0" prefWidth="112.0" text="Start" />
      <Button fx:id="infoButton" layoutX="250.0" layoutY="232.0" mnemonicParsing="false" onAction="#information" prefHeight="61.0" prefWidth="112.0" text="How To Play" />
      <Text fill="#54d338" layoutX="1.0" layoutY="18.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Frogger Arcade Game">
         <font>
            <Font name="Agency FB Bold" size="22.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
